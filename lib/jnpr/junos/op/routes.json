{
   "RouteSummaryTable" : {
      "rpc" : "get-route-summary-information",
      "key" : "table-name",
      "item" : "route-table",
      "view" : "RouteSummaryView"
   },
   "RouteTableView" : {
      "groups" : {
         "entry" : "rt-entry"
      },
      "fields_entry" : {
         "via" : "nh/via | nh/nh-local-interface",
         "protocol" : "protocol-name",
         "age" : {
            "age/@seconds" : "int"
         },
         "nexthop" : "nh/to"
      }
   },
   "_rspView" : {
      "fields" : {
         "count" : {
            "protocol-route-count" : "int"
         },
         "active" : {
            "active-route-count" : "int"
         }
      }
   },
   "RouteSummaryView" : {
      "fields" : {
         "proto" : "_rspTable",
         "holddown" : {
            "holddown-route-count" : "int"
         },
         "total" : {
            "total-route-count" : "int"
         },
         "active" : {
            "active-route-count" : "int"
         },
         "hidden" : {
            "hidden-route-count" : "int"
         },
         "dests" : {
            "destination-count" : "int"
         }
      }
   },
   "RouteTable" : {
      "args_key" : "destination",
      "key" : "rt-destination",
      "rpc" : "get-route-information",
      "view" : "RouteTableView",
      "item" : "route-table/rt"
   },
   "_rspTable" : {
      "key" : "protocol-name",
      "item" : "protocols",
      "view" : "_rspView"
   }
}
