         "distributing" : {
            "lacp-distributing" : "True=Yes"
         },
         "expired" : {
            "lacp-expired" : "True=Yes"
         },
         "aggregation" : {
            "lacp-aggregation" : "True=Yes"
         },
         "timeout" : "lacp-timeout",
         "sync" : {
            "lacp-synchronization" : "True=Yes"
         },
         "collecting" : {
            "lacp-collecting" : "True=Yes"
         },
         "activity" : "lacp-activity"
      }
   },
   "_LacpPortStateTable" : {
      "key" : [
         "name",
         "lacp-role"
      ],
