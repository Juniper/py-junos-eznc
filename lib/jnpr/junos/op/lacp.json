{
   "LacpPortView" : {
      "fields" : {
         "state" : "_LacpPortStateTable",
         "proto" : "_LacpPortProtoTable"
      }
   },
   "_LacpPortProtoTable" : {
      "item" : "lag-lacp-protocol",
      "view" : "_LacpPortProtoView"
   },
   "_LacpPortStateView" : {
      "fields" : {
         "defaulted" : {
