{
   "_fpc_hw_view" : {
      "fields" : {
         "model" : "model-number",
         "ver" : "version",
         "sn" : "serial-number",
         "pn" : "part-number",
         "desc" : "description"
      }
   },
   "FpcInfoTable" : {
      "view" : "_fpc_info_view",
      "key" : "slot",
