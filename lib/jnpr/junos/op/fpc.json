   "_fpc_info_view" : {
      "fields" : {
         "state" : "state",
         "memory" : "memory-heap-utilization",
         "cpu" : "cpu-total"
      }
   },
   "FpcHwTable" : {
      "view" : "_fpc_hw_view",
